{
	"search.exclude": {
		"**/node_modules": true,
		"**/dist": true,
		"**/coverage": true
	},
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": true
	},
	"eslint.alwaysShowStatus": true,
	"eslint.validate": ["javascript", "typescript"]
}
