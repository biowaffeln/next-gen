{
	"name": "@biowaffeln/next-gen",
	"version": "0.0.3",
	"description": "The Next.js Codebase Generator",
	"repository": {
		"type": "git",
		"url": "https://github.com/biowaffeln/next-gen.git"
	},
	"bugs": "https://github.com/biowaffeln/next-gen/issues",
	"main": "src/main.js",
	"files": [
		"dist"
	],
	"bin": {
		"next-gen": "dist/bin/next-gen.js",
		"@biowaffeln/next-gen": "dist/bin/next-gen.js"
	},
	"scripts": {
		"build": "tsc",
		"build:watch": "tsc -w",
		"postbuild": "shx cp -rf template dist",
		"prepublishOnly": "npm run build",
		"test": "tap",
		"check-deps": "node -r ts-node/register scripts/check-deps.ts"
	},
	"keywords": [
		"Next.js",
		"jscodeshift",
		"codegen",
		"scaffolding"
	],
	"author": "Mark Kvetny @biowaffeln",
	"license": "ISC",
	"dependencies": {
		"ansi-colors": "^4.1.1",
		"deepmerge": "^4.2.2",
		"fs-extra": "^9.1.0",
		"jscodeshift": "^0.11.0",
		"jscodeshift-add-imports": "^1.0.10",
		"prompts": "^2.4.0",
		"yargs": "^16.2.0"
	},
	"devDependencies": {
		"@types/fs-extra": "^9.0.6",
		"@types/jscodeshift": "^0.7.2",
		"@types/node": "^14.14.25",
		"@types/prompts": "^2.0.9",
		"@types/tap": "^14.10.2",
		"@types/test-console": "^1.1.0",
		"@types/yargs": "^16.0.0",
		"husky": "^4.3.8",
		"lint-staged": "^10.5.3",
		"node-fetch": "^2.6.1",
		"prettier": "^2.2.1",
		"semver": "^7.3.4",
		"shx": "^0.3.3",
		"tap": "^14.11.0",
		"test-console": "^1.1.0",
		"ts-node": "^9.1.1",
		"typescript": "^4.1.3"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,ts}": "prettier --write"
	}
}
